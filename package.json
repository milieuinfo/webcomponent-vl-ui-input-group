{
  "name": "vl-ui-input-group",
  "version": "1.0.8",
  "description": "Gebruik de 'input group' component om een 'input field' en een 'input add-on' te combineren. Bijvoorbeeld: de 'vl-datepicker' component combineert een 'input field' en een 'input add-on' in een 'input group'.",
  "main": "vl-input-group.js",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-input-group.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-input-group.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-input-group/style.css' vl-input-group.js",
    "release:prepare:commit": "git add -f vl-input-group.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-core": "1.4.3"
  },
  "peerDependencies": {
    "vl-ui-core": "^1.4.2"
  },
  "devDependencies": {
    "@govflanders/vl-ui-input-group": "3.7.2",
    "http-server": "0.11.1",
    "np": "5.0.3",
    "replace": "1.1.0",
    "uglify-js-es6": "2.8.9",
    "vl-ui-button": "1.0.10",
    "vl-ui-form-message": "2.1.1",
    "vl-ui-icon": "2.0.5",
    "vl-ui-input-field": "1.0.4",
    "vl-ui-util": "^1.2.3",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-input-group.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-input-group/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-input-group#readme"
}
